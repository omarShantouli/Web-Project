@{
    ViewData["Title"] = ViewBag.student.Name ;
    ViewData["Subtitle"] = ViewBag.student.Name ;
    ViewData["PageOne"] = "Logout";
}

<div class="text-center">
    
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Course Name</th>
                <th scope="col">Description</th>
                <th scope="col">Submiting Time</th>
                <th scope="col">Max Score</th>
                <th scope="col">Section Name</th>
            </tr>
        </thead>
        <tbody>
            @{
                int i = 1;
                foreach(Assignments assignment in ViewBag.assignments)
                {
                    <tr>
                        <th scope="row">@i</th>
                        <th scope="row">@assignment.CourseName</th>
                        <td>@assignment.Description</td>
                        <td>@assignment.SubmitingTime</td>
                        <td>@assignment.MaxScore</td>
                        <td>@ViewBag.sections[@assignment.SecionId]</td>
                    </tr>
                    
                    i++;
                }
            }
        </tbody>
     </table> 


</div>
